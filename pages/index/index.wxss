.swipers swiper {
  width: 750rpx;
  height: 340rpx;
}
.swipers swiper image {
  width: 100%;
}
.categorys {
  display: flex;
}
.categorys navigator {
  flex: 1;
  padding: 20rpx;
}
.categorys navigator image {
  width: 100%;
}
.floor .floor_group {
  padding: 10rpx 0rpx;
}
.floor .floor_group .floor_title image {
  width: 100%;
}
.floor .floor_group .floor_list {
  overflow: hidden;
}
.floor .floor_group .floor_list navigator {
  float: left;
  width: 33.3%;
  /*宽高继承父元素*/
}
.floor .floor_group .floor_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #FFFFFF;
}
.floor .floor_group .floor_list navigator:nth-child(2),
.floor .floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #FFFFFF;
}
.floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
